                    <gml:identifier codeSpace="ISOTC211/19115">application</gml:identifier>
                </CodeDefinition>
            </codeEntry>
        </CodeListDictionary>
    </codelistItem>
    
    <!--=== EOF ===-->
</CT_CodelistCatalogue>