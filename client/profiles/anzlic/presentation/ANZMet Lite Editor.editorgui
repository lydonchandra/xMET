  &lt;/xsl:template&gt;

  &lt;xsl:template match="//gmd:DQ_DataQuality" priority="1000" mode="M11"&gt;
    &lt;xsl:if test="(((count(*/gmd:LI_Lineage/gmd:source) +     count(*/gmd:LI_Lineage/gmd:processStep)) = 0) and     (gmd:scope/gmd:DQ_Scope/gmd:level/gmd:MD_ScopeCode/@codeListValue='dataset'     or     gmd:scope/gmd:DQ_Scope/gmd:level/gmd:MD_ScopeCode/@codeListValue='series'))     and not(gmd:lineage/gmd:LI_Lineage/gmd:statement) and (gmd:lineage)"&gt;
      &lt;xsl:element name="alertUser"&gt;
        &lt;xsl:element name="title"&gt;Error&lt;/xsl:element&gt;
        &lt;xsl:element name="message"&gt;
          In History and Quality, if Scope is "dataset" or "series," History is mandatory
          &lt;xsl:value-of select="string('&amp;#xA;')"/&gt;
        &lt;/xsl:element&gt;
      &lt;/xsl:element&gt;
    &lt;/xsl:if&gt;
    &lt;xsl:apply-templates select="*|comment()|processing-instruction()" mode="M11"/&gt;
  &lt;/xsl:template&gt;
  &lt;xsl:template match="text()" priority="-1" mode="M11"/&gt;
  &lt;xsl:template match="@*|node()" priority="-2" mode="M11"&gt;
    &lt;xsl:apply-templates select="*|comment()|processing-instruction()" mode="M11"/&gt;
  &lt;/xsl:template&gt;

  &lt;!-- 
  &lt;xsl:template match="//gmd:LI_Lineage" priority="1000" mode="M12"&gt;
    &lt;xsl:if test="not(gmd:source) and not(gmd:statement) and not(gmd:processStep)"&gt;
      &lt;xsl:element name="alertUser"&gt;
        &lt;xsl:element name="title"&gt;Error&lt;/xsl:element&gt;
        &lt;xsl:element name="message"&gt;
          "source" role is mandatory if LI_Lineage.statement and "processStep" role are not documented&lt;xsl:value-of select="string('&amp;#xA;')"/&gt;
        &lt;/xsl:element&gt;
      &lt;/xsl:element&gt;
    &lt;/xsl:if&gt;
    &lt;xsl:apply-templates select="*|comment()|processing-instruction()" mode="M12"/&gt;
  &lt;/xsl:template&gt;
  &lt;xsl:template match="text()" priority="-1" mode="M12"/&gt;
  &lt;xsl:template match="@*|node()" priority="-2" mode="M12"&gt;
    &lt;xsl:apply-templates select="*|comment()|processing-instruction()" mode="M12"/&gt;
  &lt;/xsl:template&gt;

  
  &lt;xsl:template match="//gmd:LI_Lineage" priority="1000" mode="M13"&gt;
    &lt;xsl:if test="not(gmd:processStep) and not(gmd:statement) and not(gmd:source)"&gt;
      &lt;xsl:element name="alertUser"&gt;
        &lt;xsl:element name="title"&gt;Error&lt;/xsl:element&gt;
        &lt;xsl:element name="message"&gt;
          "processStep" role is mandatory if LI_Lineage.statement and "source" role are not documented&lt;xsl:value-of select="string('&amp;#xA;')"/&gt;
        &lt;/xsl:element&gt;
      &lt;/xsl:element&gt;
    &lt;/xsl:if&gt;
    &lt;xsl:apply-templates select="*|comment()|processing-instruction()" mode="M13"/&gt;
  &lt;/xsl:template&gt;
  &lt;xsl:template match="text()" priority="-1" mode="M13"/&gt;
  &lt;xsl:template match="@*|node()" priority="-2" mode="M13"&gt;
    &lt;xsl:apply-templates select="*|comment()|processing-instruction()" mode="M13"/&gt;
  &lt;/xsl:template&gt;
--&gt;

  &lt;xsl:template match="//gmd:DQ_DataQuality" priority="1000" mode="M14"&gt;
    &lt;!--
      REPORT --&gt;
    &lt;xsl:if test="gmd:scope/gmd:DQ_Scope/gmd:level/gmd:MD_ScopeCode/@codeListValue='dataset' and      not(gmd:report) and      not(gmd:lineage)"&gt;
      &lt;xsl:element name="alertUser"&gt;
        &lt;xsl:element name="title"&gt;Error&lt;/xsl:element&gt;
        &lt;xsl:element name="message"&gt;
          In History and Quality, if Scope is "dataset," Quality Report or History must be provided
        &lt;/xsl:element&gt;
      &lt;/xsl:element&gt;
    &lt;/xsl:if&gt;
    &lt;xsl:apply-templates select="*|comment()|processing-instruction()" mode="M14"/&gt;
  &lt;/xsl:template&gt;
  &lt;xsl:template match="text()" priority="-1" mode="M14"/&gt;
  &lt;xsl:template match="@*|node()" priority="-2" mode="M14"&gt;
    &lt;xsl:apply-templates select="*|comment()|processing-instruction()" mode="M14"/&gt;
  &lt;/xsl:template&gt;

  &lt;xsl:template match="//gmd:MD_Distribution" priority="1000" mode="M17"&gt;
    &lt;xsl:choose&gt;
      &lt;xsl:when test="count(gmd:distributionFormat)&amp;gt;0 or count(gmd:distributor/gmd:MD_Distributor/gmd:distributorFormat)&amp;gt;0"/&gt;
      &lt;xsl:otherwise&gt;
        &lt;xsl:element name="alertUser"&gt;
          &lt;xsl:element name="title"&gt;Error&lt;/xsl:element&gt;
          &lt;xsl:element name="message"&gt;
            In Distribution, at least one Distribution Format must be specified.
          &lt;/xsl:element&gt;
        &lt;/xsl:element&gt;
      &lt;/xsl:otherwise&gt;
    &lt;/xsl:choose&gt;
    &lt;xsl:apply-templates select="*|comment()|processing-instruction()" mode="M17"/&gt;
  &lt;/xsl:template&gt;
  &lt;xsl:template match="text()" priority="-1" mode="M17"/&gt;
  &lt;xsl:template match="@*|node()" priority="-2" mode="M17"&gt;
    &lt;xsl:apply-templates select="*|comment()|processing-instruction()" mode="M17"/&gt;
  &lt;/xsl:template&gt;

  &lt;xsl:template match="//gmd:EX_Extent" priority="1000" mode="M18"&gt;
    &lt;xsl:choose&gt;
      &lt;xsl:when test="count(gmd:description)&amp;gt;0 or count(gmd:geographicElement)&amp;gt;0 or count(gmd:temporalElement)&amp;gt;0 or count(gmd:verticalElement)&amp;gt;0"/&gt;
      &lt;xsl:otherwise&gt;
        &lt;xsl:element name="alertUser"&gt;
          &lt;xsl:element name="title"&gt;Error&lt;/xsl:element&gt;
          &lt;xsl:element name="message"&gt;
            An Extent must either have a description or a Geographic Extent or a Temporal Extent or a Vertical Extent.
          &lt;/xsl:element&gt;
        &lt;/xsl:element&gt;
      &lt;/xsl:otherwise&gt;
    &lt;/xsl:choose&gt;
    &lt;xsl:apply-templates select="*|comment()|processing-instruction()" mode="M18"/&gt;
  &lt;/xsl:template&gt;
  &lt;xsl:template match="text()" priority="-1" mode="M18"/&gt;
  &lt;xsl:template match="@*|node()" priority="-2" mode="M18"&gt;
    &lt;xsl:apply-templates select="*|comment()|processing-instruction()" mode="M18"/&gt;
  &lt;/xsl:template&gt;


  &lt;xsl:template match="//*[gmd:identificationInfo/gmd:MD_DataIdentification]" priority="1000"
                   mode="M20"&gt;
    &lt;xsl:if test="(not(gmd:hierarchyLevel) or gmd:hierarchyLevel/gmd:MD_ScopeCode/@codeListValue='dataset') and      (count(//gmd:MD_DataIdentification/gmd:extent/*/gmd:geographicElement/gmd:EX_GeographicBoundingBox) + count (//gmd:MD_DataIdentification/gmd:extent/*/gmd:geographicElement/gmd:EX_GeographicDescription)) =0 "&gt;
      &lt;xsl:element name="alertUser"&gt;
        &lt;xsl:element name="title"&gt;Error&lt;/xsl:element&gt;
        &lt;xsl:element name="message"&gt;
          If Reosource type is dataset, at least one Bounding Box or a Geographic Identifier must be present in Geographic Extent.
        &lt;/xsl:element&gt;
      &lt;/xsl:element&gt;
    &lt;/xsl:if&gt;
    &lt;xsl:apply-templates select="*|comment()|processing-instruction()" mode="M20"/&gt;
  &lt;/xsl:template&gt;
  &lt;xsl:template match="text()" priority="-1" mode="M20"/&gt;
  &lt;xsl:template match="@*|node()" priority="-2" mode="M20"&gt;
    &lt;xsl:apply-templates select="*|comment()|processing-instruction()" mode="M20"/&gt;
  &lt;/xsl:template&gt;

  &lt;xsl:template match="//gmd:MD_DataIdentification" priority="1000" mode="M21"&gt;
    &lt;xsl:if test="(not(../../gmd:hierarchyLevel) or     (../../gmd:hierarchyLevel/gmd:MD_ScopeCode/@codeListValue='dataset') or     (../../gmd:hierarchyLevel/gmd:MD_ScopeCode/@codeListValue='series')) and     (not(gmd:topicCategory))"&gt;
      &lt;xsl:element name="alertUser"&gt;
        &lt;xsl:element name="title"&gt;Error&lt;/xsl:element&gt;
        &lt;xsl:element name="message"&gt;
          If Rsource Type is dataset or series, at least one Topic Catagory must be provided
        &lt;/xsl:element&gt;
      &lt;/xsl:element&gt;
    &lt;/xsl:if&gt;
    &lt;xsl:apply-templates select="*|comment()|processing-instruction()" mode="M21"/&gt;
  &lt;/xsl:template&gt;
  &lt;xsl:template match="text()" priority="-1" mode="M21"/&gt;
  &lt;xsl:template match="@*|node()" priority="-2" mode="M21"&gt;
    &lt;xsl:apply-templates select="*|comment()|processing-instruction()" mode="M21"/&gt;
  &lt;/xsl:template&gt;

  &lt;!-- 
    &lt;xsl:template match="gmd:MD_Keywords/gmd:keyword/gco:CharacterString" priority="1000"
                   mode="M6"&gt;
    &lt;xsl:variable name="URI-reference"
                  select="../../gmd:thesaurusName/gmd:CI_Citation/gmd:identifier/gmd:MD_Identifier/gmd:code/gco:CharacterString"/&gt;
    &lt;xsl:variable name="URI" select="substring-before( $URI-reference, '#')"/&gt;
    &lt;xsl:variable name="fragment" select="substring-after( $URI-reference, '#')"/&gt;
    &lt;xsl:variable name="code-list-document" select="document( $URI )"/&gt;
    &lt;xsl:variable name="dictionary"
                  select=" $code-list-document//gmx:CodeListDictionary[@gml:id = $fragment ]"/&gt;

    &lt;xsl:if test=" ($fragment='osdm-schedule') and not($code-list-document)"&gt;
      &lt;xsl:element name="alertUser"&gt;
        &lt;xsl:element name="title"&gt;Error&lt;/xsl:element&gt;
        &lt;xsl:element name="message"&gt;
          Can't find the OSDM schedule code list at &lt;xsl:text/&gt;
          &lt;xsl:value-of select="$URI"/&gt;
          &lt;xsl:text/&gt;
          &lt;xsl:value-of select="string('&amp;#xA;')"/&gt;
        &lt;/xsl:element&gt;
      &lt;/xsl:element&gt;
    &lt;/xsl:if&gt;
    &lt;xsl:variable name="hierarchy"
                  select="//*/gmd:hierarchyLevel/gmd:MD_ScopeCode/@codeListValue"/&gt;
    &lt;xsl:variable name="jurisdiction" select="current()"/&gt;
    &lt;xsl:variable name="osdmCodeList"
                  select="'http://asdd.ga.gov.au/asdd/profileinfo/osdm-schedule.xml'"/&gt;
    &lt;xsl:variable name="schedulePath"
                  select="//*/gmd:descriptiveKeywords/gmd:MD_Keywords/gmd:thesaurusName/gmd:CI_Citation/gmd:identifier/gmd:MD_Identifier/gmd:code/gco:CharacterString"/&gt;
    &lt;xsl:variable name="scheduleOK"
                  select="//*         [contains(gmd:MD_Keywords/gmd:thesaurusName/gmd:CI_Citation/gmd:identifier/gmd:MD_Identifier/gmd:code/gco:CharacterString, '#osdm-schedule')]"/&gt;

    &lt;xsl:if test="(not($hierarchy) or $hierarchy='dataset')          and ($fragment = 'anzlic-jurisdic')         and ($jurisdiction = 'Australia')         and (not($scheduleOK))         "&gt;
      &lt;xsl:element name="alertUser"&gt;
        &lt;xsl:element name="title"&gt;Error&lt;/xsl:element&gt;
        &lt;xsl:element name="message"&gt;
          OSDM requires an Australian Government 'dataset' to have an OSDM Schedule rating where:
          &lt;xsl:text/&gt;hierarchyLevel&lt;xsl:text/&gt; = '&lt;xsl:text/&gt;
          &lt;xsl:value-of select="$hierarchy"/&gt;
          &lt;xsl:text/&gt;' and
          &lt;xsl:text/&gt;jurisdiction keyword&lt;xsl:text/&gt; = '&lt;xsl:text/&gt;
          &lt;xsl:value-of select="current()"/&gt;
          &lt;xsl:text/&gt;'.
          &lt;xsl:text/&gt;Note:&lt;xsl:text/&gt; that the
          &lt;xsl:text/&gt;dictionary list&lt;xsl:text/&gt; '&lt;xsl:text/&gt;
          &lt;xsl:value-of select="$schedulePath"/&gt;
          &lt;xsl:text/&gt;'
          does not contain the
          &lt;xsl:text/&gt;OSDM schedule dictionary&lt;xsl:text/&gt; 'osdm-schedule'
          &lt;xsl:value-of select="string('&amp;#xA;')"/&gt;
        &lt;/xsl:element&gt;
      &lt;/xsl:element&gt;
    &lt;/xsl:if&gt;
    &lt;xsl:if test="not($dictionary/gmx:codeEntry/gmx:CodeDefinition[gml:identifier = current()]) and        not($dictionary/gmx:codeEntry/gmx:CodeDefinition[gml:description = current()])       and ($fragment='osdm-schedule')"&gt;
      &lt;xsl:element name="alertUser"&gt;
        &lt;xsl:element name="title"&gt;Error&lt;/xsl:element&gt;
        &lt;xsl:element name="message"&gt;
          Could not find the keyword code "&lt;xsl:text/&gt;
          &lt;xsl:value-of select="current()"/&gt;
          &lt;xsl:text/&gt;"
          in the keyword code list
          "&lt;xsl:text/&gt;
          &lt;xsl:value-of select="$URI"/&gt;
          &lt;xsl:text/&gt;#&lt;xsl:text/&gt;
          &lt;xsl:value-of select="$fragment"/&gt;
          &lt;xsl:text/&gt;"
          &lt;xsl:value-of select="string('&amp;#xA;')"/&gt;
        &lt;/xsl:element&gt;
      &lt;/xsl:element&gt;
    &lt;/xsl:if&gt;
    &lt;xsl:apply-templates select="*|comment()|processing-instruction()" mode="M6"/&gt;
  &lt;/xsl:template&gt;
  &lt;xsl:template match="text()" priority="-1" mode="M6"/&gt;
  &lt;xsl:template match="@*|node()" priority="-2" mode="M6"&gt;
    &lt;xsl:apply-templates select="*|comment()|processing-instruction()" mode="M6"/&gt;
  &lt;/xsl:template&gt;
  
  --&gt;
&lt;/xsl:stylesheet&gt;</validationXSL>
</gui>