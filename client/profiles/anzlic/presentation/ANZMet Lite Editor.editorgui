                  </codeBlock>
                </code>
                <elseCode>
                  <if>
                    <base>$/gmd:DQ_DataQuality/gmd:report</base>
                    <condition>present</condition>
                    <code>
                      <codeBlock>
                        <setValid>
                          <name>localScopeVar</name>
                        </setValid>
                      </codeBlock>
                    </code>
                    <elseCode>
