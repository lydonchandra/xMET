      </item>
      <visible>true</visible>
      <onValidation>
        <codeBlock>
          <if>
            <base>%localScopeVar</base>
            <condition>equals</condition>
            <expression>dataset</expression>
            <code>
              <if>
                <base>$/gmd:DQ_DataQuality/gmd:lineage</base>
                <condition>present</condition>
                <code>
                  <codeBlock>
                    <setValid>
                      <name>localScopeVar</name>
